{
  "name": "vue-flow-grid",
  "version": "1.0.0",
  "description": "A simple flow grid component for Vue.js",
  "main": "index.js",
  "scripts": {
    "watch-compile": "rollup -c ./rollup.config.js -w",
    "serve": "browser-sync start --server --files \"dist/*.js, *.html\"",
    "build": "rollup -c ./rollup.config.js",
    "develop": "run-p watch-compile serve",
    "start": "run-s build develop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/75team-biz/vue-flow-grid.git"
  },
  "keywords": [
    "vue",
    "flow",
    "grid",
    "flowgrid"
  ],
  "author": "NangeGao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/75team-biz/vue-flow-grid/issues"
  },
  "homepage": "https://github.com/75team-biz/vue-flow-grid#readme",
  "devDependencies": {
    "browser-sync": "^2.18.8",
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "deep-diff": "^0.3.4"
  }
}
